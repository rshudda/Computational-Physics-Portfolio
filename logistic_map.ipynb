    "\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def logistic(r,x,n):\n",
    "    xs=[x]\n",
    "    for _ in range(n):\n",
    "        x=r*x*(1-x)\n",
    "        xs.append(x)\n",
    "    return np.array(xs)\n",
    "\n",
    "r=3.5; x0=0.2\n",
    "xs = logistic(r,x0,200)\n",
    "plt.plot(xs)\n",
    "plt.title(\"Logistic Map\")\n",
    "plt.show()\n"
   ]
  }
